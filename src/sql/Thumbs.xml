<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.moxi.dao.Thumbs.IThumbsDao" >

	<!-- 保存点赞 -->
	<insert id="saveNewsThumbs" parameterType="com.moxi.model.NewsThumbs">
		INSERT INTO app_v2_news_thumbs (newsId, userId)
		VALUES
			(#{newsId}, #{userId})
	</insert>
	
	
</mapper>